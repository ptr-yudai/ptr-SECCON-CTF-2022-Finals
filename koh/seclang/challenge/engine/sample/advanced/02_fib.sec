func fib(n) {
  if n <= 0 { return 0; }
  if n == 1 { return 1; }
  if n == 2 { return 1; }
  return fib(n-1) + fib(n-2);
}

func main() {
  print("n = ");
  n = scan();
  if n > 20 {
    print("n is too big!\n");
    exit(1);
  }
  print("fib(");
  print(n);
  print(") = ");
  print(fib(n));
  print("\n");
}
