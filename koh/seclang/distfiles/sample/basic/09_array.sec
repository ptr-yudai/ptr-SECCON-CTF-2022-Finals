func main() {
  arr = [314, 159, 265, 358, 979, 323, 846, 264, 338, 327];
  print(arr);
  print("\n");

  i = 0;
  j = 0;
  while i < 10 {
    if arr[i] % 2 == 0 {
      arr[j] = arr[i];
      j = j + 1;
    }
    i = i + 1;
  }
  while j < 10 {
    arr[j] = -1;
    j = j + 1;
  }

  print(arr);
  print("\n");
  return 0;
}
